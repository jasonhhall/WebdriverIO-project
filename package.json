{
    "name": "webdriverio-tests",
    "version": "0.1.0",
    "private": true,
    "main": "./node_modules/.bin/wdio",
    "scripts": {
        "merge": "node mergeResults.js ./Results 'wdio-mochawesome-*'",
        "generateMochawesome":"marge ./Results --reportTitle 'My Project Results'",
        "chrome": "npx wdio run ./test/config/chrome.conf.js",
        "remote": "npx wdio run ./test/config/remote.conf.js",
        "spec": "npx wdio run ./test/config/chrome.conf.js --spec",
        "suite": "npx wdio run ./test/config/chrome.conf.js --suite",
        "selenium": "npx wdio run ./test/config/selenium-standalone.conf.js",
        "docker:up": "docker-compose up",
        "docker:selenium": "docker run -it -p 4444:4444 webdriverio/selenium-standalone",
        "selenium:standalone": "npx selenium-standalone install && npx selenium-standalone start"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.18.0",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/plugin-proposal-class-properties": "^7.17.12",
        "@babel/preset-env": "^7.18.0",
        "@babel/register": "^7.17.7",
        "@delta-reporter/wdio-delta-reporter-service": "^1.1.10",
        "@wdio/dot-reporter": "^7.19.7",
        "@wdio/junit-reporter": "^7.19.7",
        "@wdio/local-runner": "^7.19.7",
        "@wdio/mocha-framework": "^7.19.7",
        "@wdio/selenium-standalone-service": "^7.19.5",
        "@wdio/spec-reporter": "^7.19.7",
        "dotenv": "^16.0.1",
        "junit-viewer": "^4.11.1",
        "mochawesome-report-generator": "^6.2.0",
        "wdio-chromedriver-service": "^7.3.2",
        "wdio-json-reporter": "^3.0.0",
        "wdio-mochawesome-reporter": "^4.0.0"
    },
    "dependencies": {
        "@wdio/cli": "^7.19.7",
        "allure-commandline": "^2.18.1",
        "mocha": "^10.0.0",
        "selenium-standalone": "^8.1.4"
    }
}
