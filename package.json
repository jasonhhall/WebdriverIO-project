{
    "name": "webdriverio-tests",
    "version": "0.1.0",
    "private": true,
    "main": "./node_modules/.bin/wdio",
    "scripts": {
        "chrome": "npx wdio run ./test/config/chrome.conf.js",
        "local": "npx wdio run ./test/config/local.conf.js",
        "spec": "npx wdio run ./test/config/wdio.local.conf.js --spec",
        "suite": "npx wdio run ./test/config/wdio.local.conf.js --suite",
        "selenium": "npx wdio run ./test/config/wdio.selenium.conf.js --spec",
        "docker:up": "docker-compose up",
        "docker": "docker run -it -p 4444:4444 webdriverio/selenium-standalone",
        "selenium:standalone": "npx selenium-standalone install && npx selenium-standalone start",
        "html": "wdio-junit-to-html -i ./test/reports/junit-results -o ./test/reports/html"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.18.0",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/plugin-proposal-class-properties": "^7.17.12",
        "@babel/preset-env": "^7.18.0",
        "@babel/register": "^7.17.7",
        "@delta-reporter/wdio-delta-reporter-service": "^1.1.10",
        "@wdio/cli": "^7.19.7",
        "@wdio/dot-reporter": "^7.19.7",
        "@wdio/junit-reporter": "^7.19.7",
        "@wdio/local-runner": "^7.19.7",
        "@wdio/mocha-framework": "^7.19.7",
        "@wdio/selenium-standalone-service": "^7.19.5",
        "@wdio/spec-reporter": "^7.19.7",
        "dotenv": "^16.0.1"
     
    },
    "dependencies": {
        
        "mocha": "^10.0.0",
        "selenium-standalone": "^8.1.4"
    }
}
